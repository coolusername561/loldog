-- Scripts:

local function IZDX_fake_script() -- MainFrame.main 
	local script = Instance.new('LocalScript', MainFrame)

	local frame = script.Parent
	
	frame.Active = true
	frame.Draggable = true
	
	local UIS = game:GetService("UserInputService")
	
	UIS.InputBegan:Connect(function(input, gp)
		if input.KeyCode == Enum.KeyCode.RightShift then
			frame.Visible = not frame.Visible
		end
	end)
end
coroutine.wrap(IZDX_fake_script)()
local function GPPEF_fake_script() -- MainFrame.tabhandler 
	local script = Instance.new('LocalScript', MainFrame)

	local tabs = {
		Home = {
			Button = script.Parent.SidePanel:WaitForChild("Home"),
			Frame = script.Parent:WaitForChild("homeframe")
		},
		Player = {
			Button = script.Parent.SidePanel:WaitForChild("Scripts"),
			Frame = script.Parent:WaitForChild("scriptsframe")
		},
		Visuals = {
			Button = script.Parent.SidePanel:WaitForChild("Universal"),
			Frame = script.Parent:WaitForChild("universalframe")
		}
	}
	
	local inactiveColor = Color3.fromRGB(20,20,20)
	local activeColor   = Color3.fromRGB(25,25,25)
	
	local function switchTab(tabName)
		for name, data in pairs(tabs) do
			if name == tabName then
				data.Button.BackgroundColor3 = activeColor
				data.Frame.Visible = true
			else
				data.Button.BackgroundColor3 = inactiveColor
				data.Frame.Visible = false
			end
		end
	end
	
	for name, data in pairs(tabs) do
		data.Button.MouseButton1Click:Connect(function()
			switchTab(name)
		end)
	end
	
	switchTab("Home")
end
coroutine.wrap(GPPEF_fake_script)()
