        loadstring(
            game:HttpGet(
                'https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua'
            )
        )()
    end)

    local gradient = button:FindFirstChildOfClass('UIGradient')
    if not gradient then
        gradient = Instance.new('UIGradient')
        gradient.Parent = button
    end

    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(0.45, Color3.fromRGB(230, 230, 230)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(0.55, Color3.fromRGB(230, 230, 230)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 255, 255)),
    })

    gradient.Rotation = 45
    gradient.Offset = Vector2.new(-1, -1)

    while true do
        for i = -1, 1, 0.02 do
            gradient.Offset = Vector2.new(i, i)
            task.wait(0.03)
        end
    end
end
coroutine.wrap(ZXUTW_fake_script)()
