local function MFUMNE_fake_script() -- TextButton.openclose 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local mainFrame = button.Parent:WaitForChild("MainFrame")
	
	local isOpen = true  
	mainFrame.Visible = true  
	button.Text = "Close UI"
	
	button.MouseButton1Click:Connect(function()
		if isOpen then
			mainFrame.Visible = false
			button.Text = "Open UI"
		else
			mainFrame.Visible = true
			button.Text = "Close UI"
		end
		isOpen = not isOpen
	end)
end
coroutine.wrap(MFUMNE_fake_script)()
